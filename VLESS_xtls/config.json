{
    "log": {
        "access": "/var/log/xray/access.log",
        "error": "/var/log/xray/error.log",
        "loglevel": "warning"
    },
    "inbound": {
        "port": 443,
        "protocol": "vless",
        "settings": {
            "clients": [{
                "id": "UUID",
                "flow": "xtls-rprx-direct",
                "level": 0,
                "email": "me@idleleo.com"
            }],
            "decryption": "none",
            "fallbacks": [{
                "dest": 8080,
                "xver": 1
            }]
        },
        "streamSettings": {
            "network": "tcp",
            "security": "xtls",
            "xtlsSettings": {
                "alpn": [
                    "http/1.1"
                ],
                "certificates": [{
                    "certificateFile": "/data/xray.crt",
                    "keyFile": "/data/xray.key"
                }]
            }
        }
    },
    "outbound": {
        "protocol": "freedom",
        "settings": {}
    },
    "dns": {
        "servers": [
            "https+local://1.1.1.1/dns-query",
            "1.1.1.1",
            "1.0.0.1",
            "8.8.8.8",
            "8.8.4.4",
            "localhost"
        ]
    },
    "routing": {
        "domainStrategy": "AsIs",
        "rules": [{
            "type": "field",
            "outboundTag": "direct"
        }]
    }
}